{
  "name": "field-guide",
  "version": "2.3.0",
  "description": "a fully-functional, static site implementation of a design system documentation site",
  "keywords": [
    "ember-addon",
    "prember-plugin",
    "empress",
    "field-guide"
  ],
  "repository": "https://github.com/empress/field-guide",
  "license": "MIT",
  "author": "Chris Manson <chris@manson.ie>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "ember-compatibility-test": "ember try:each"
  },
  "dependencies": {
    "@embroider/util": "^1.6.0",
    "@glimmer/component": "^1.0.4",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-static-site-json": "^4.4.1",
    "ember-auto-import": "^1.11.3 || ^2.0.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-showdown": "^6.0.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-fetch": "^8.0.1",
    "ember-get-config": "^2.0.0",
    "ember-html-excerpt": "^0.2.0",
    "ember-meta": "^2.0.0",
    "ember-prism": "^0.10.0",
    "empress-blueprint-helpers": "^1.2.1",
    "innertext": "^1.0.3",
    "resolve": "^1.17.0",
    "walk-sync": "^2.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.6.0",
    "@glimmer/tracking": "^1.0.4",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.5",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^3.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-test-selectors": "^6.0.0",
    "ember-try": "https://git@github.com/mansona/ember-try.git#npm-overrides",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "field-guide-default-template": "^2.0.0",
    "lint-to-the-future": "^0.6.0",
    "lint-to-the-future-eslint": "^0.2.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prember": "^1.1.0",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "webpack": "^5.52.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-meta"
  }
}
