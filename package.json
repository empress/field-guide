{
  "name": "field-guide",
  "version": "2.4.0",
  "description": "a fully-functional, static site implementation of a design system documentation site",
  "keywords": [
    "ember-addon",
    "prember-plugin",
    "empress",
    "field-guide"
  ],
  "repository": "https://github.com/empress/field-guide",
  "license": "MIT",
  "author": "Chris Manson <chris@manson.ie>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "ember-compatibility-test": "ember try:each"
  },
  "dependencies": {
    "@embroider/util": "^1.13.2",
    "@glimmer/component": "^1.1.2",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-static-site-json": "^4.4.1",
    "ember-auto-import": "^2.5.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-showdown": "^6.0.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-fetch": "^8.1.2",
    "ember-get-config": "^0.3.0",
    "ember-html-excerpt": "^0.2.0",
    "ember-meta": "^2.0.0",
    "ember-prism": "^0.13.0",
    "empress-blueprint-helpers": "^1.2.1",
    "innertext": "^1.0.3",
    "resolve": "^1.17.0",
    "walk-sync": "^2.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.0.2",
    "@glimmer/tracking": "^1.1.2",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-cli": "~4.10.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-fastboot": "^4.1.5",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.1.1",
    "ember-resolver": "^8.1.0",
    "ember-source": "~4.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.3.1",
    "ember-test-selectors": "^6.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.4.3",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "field-guide-default-template": "^2.0.0",
    "lint-to-the-future": "^0.6.0",
    "lint-to-the-future-eslint": "^0.2.1",
    "loader.js": "^4.7.0",
    "prember": "^2.1.0",
    "prettier": "^2.8.3",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-meta"
  }
}
